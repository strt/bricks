    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.1.3",
    "babel-loader": "^8.0.0",
    "babel-plugin-macros": "^2.4.2",
    "browser-sync": "^2.24.7",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "^2.4.1",
