  "license": "MIT",
  "repository": "github:strt/bricks",
  "keywords": [
    "bricks",
    "toolkit",
    "0CJS",
    "zero-configuration"
  ],
  "engines": {
    "node": ">=8"
  },
  "files": [
    "babel.js",
    "bin",
    "config",
    "scripts",
    "tasks",
    "utils"
